{
	"services":{
		"devtools_store":{
			"type":"html_assets",
			"routes":[
				{
					"route":"/store",
					"directory":"./apps/public",
					"default_file":"store_config.html"
				},
				{
					"route":"/examples",
					"directory":"./apps/public",
					"default_file":"react_counter.html"
				}
			]
		},
		
		"devtools_panel":{
			"type":"middleware",
			"routes":[
				{
					"route":"/store/config/all",
					"mw_file":"apps/private/devtools/store/config/store_config_mw.js"
				},
				{
					"route":"/store/config/applications",
					"mw_file":"apps/private/devtools/store/config/store_config_applications_mw.js"
				},
				{
					"route":"/store/config/resources",
					"mw_file":"apps/private/devtools/store/config/store_config_resources_mw.js"
				},
				{
					"route":"/store/config/services",
					"mw_file":"apps/private/devtools/store/config/store_config_services_mw.js"
				},
				{
					"route":"/store/config/servers",
					"mw_file":"apps/private/devtools/store/config/store_config_servers_mw.js"
				},
				{
					"route":"/store/config/modules",
					"mw_file":"apps/private/devtools/store/config/store_config_modules_mw.js"
				},
				{
					"route":"/store/config/plugins",
					"mw_file":"apps/private/devtools/store/config/store_config_plugins_mw.js"
				},
				{
					"route":"/store/config/security",
					"mw_file":"apps/private/devtools/store/config/store_config_security_mw.js"
				},
				
				
				{
					"route":"/store/config/views",
					"mw_file":"apps/private/devtools/store/config/resources/store_config_views_mw.js"
				},
				{
					"route":"/store/config/models",
					"mw_file":"apps/private/devtools/store/config/resources/store_config_models_mw.js"
				},
				{
					"route":"/store/config/menus",
					"mw_file":"apps/private/devtools/store/config/resources/store_config_menus_mw.js"
				},
				{
					"route":"/store/config/menubars",
					"mw_file":"apps/private/devtools/store/config/resources/store_config_menubars_mw.js"
				},
				
				
				{
					"route":"/store/runtime",
					"mw_file":"apps/private/devtools/store/runtime/store_runtime_mw.js"
				},
				
				
				{
					"route":"/logs",
					"mw_file":"apps/private/devtools/logs/logs_mw.js"
				}
			]
		},
		
		"html_assets_1":{
			"type":"html_assets",
			"routes":[
				{
					"route":"/css/.*",
					"directory":"./apps/public"
				},
				{
					"route":"/js/.*",
					"directory":"./apps/public"
				},
				{
					"route":"/img/.*",
					"directory":"./apps/public"
				}
			]
		},
		
		"rest_api_models_query_1":{ 
			"type":"rest_api_models_query",
			"routes":[
				{
					"route":"/crud"
				}
			]
		},
		
		"rest_api_models_modifier_1":{
			"type":"rest_api_models_modifier",
			"routes":[
				{
					"route":"/crud"
				}
			]
		},
		
		"rest_api_resources_query_1":{
			"type":"rest_api_resources_query",
			"routes":[
				{
					"route":"/connexions",
					"collection":"connexions"
				},
				{
					"route":"/connexions/:name",
					"collection":"connexions",
					"item":"name"
				},
				
				{
					"route":"/views",
					"collection":"views"
				},
				{
					"route":"/views/:name",
					"collection":"views",
					"item":"name"
				},
				
				{
					"route":"/models",
					"collection":"models"
				},
				{
					"route":"/models/:name",
					"collection":"models",
					"item":"name"
				},
				
				{
					"route":"/menubars",
					"collection":"menubars"
				},
				{
					"route":"/menubars/:name",
					"collection":"menubars",
					"item":"name"
				},
				
				{
					"route":"/menus",
					"collection":"menus"
				},
				{
					"route":"/menus/:name",
					"collection":"menus",
					"item":"name"
				}
			]
		},
		
		"svc4":{
			"type":"rest_api_resources_modifier"
		},
		
		"svc6":{ "type":"html_app" }
	}
}