{
  "name": "devapt-server",
  "version": "1.0.0",
  "description": "Devapt REST API server",
  "author": "Luc BORIES (https://github.com/lucbories)",
  "license": "Apache-2.0",
  "license2": "Apache License 2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucbories/devapt.git"
  },
  "bugs": {
    "url": "git+https://github.com/lucbories/devapt/issues"
  },
  "homepage": "https://github.com/lucbories/devapt#readme.md",
  "main": "start.js",
  "scripts": {
    "start": "babel-node --debug src/server/start.js",
    "cli": "babel-node --debug src/server/cli.js",
    "start-vscode": "babel-node src/server/start.js",
    "test": "mocha --compilers js:babel/register --require ./test/test_helper.js --recursive",
    "test:watch": "npm run test -- --watch",
    "build": "webpack --progress --colors",
    "build:watch": "webpack --progress --colors --watch",
    "start:dev": "webpack-dev-server ./entry --progress --colors --hot --inline",
    "build:client": "./node_modules/.bin/babel client/src/js --out-file dist/devapt_client_babel.js",
    "build:common": "./node_modules/.bin/babel common --out-file dist/devapt_common_babel.js",
    "build:server": "./node_modules/.bin/babel server --out-file dist/devapt_server_babel.js"
  },
  "keywords": [
    "server",
    "api",
    "rest",
    "devapt",
    "toolkit",
    "redux"
  ],
  "dependencies": {
    "MD5": "^1.3.0",
    "babel-core": "^6.1.0",
    "bunyan": "^1.5.1",
    "commander": "^2.9.0",
    "debug": "^2.2.0",
    "epilogue": "^0.5.2",
    "express": "^4.13.3",
    "fbemitter": "~2.0.0",
    "immutable": "^3.7.5",
    "immutable-js": "~0.3.1-6",
    "ini": "^1.3.4",
    "moment": "^2.10.6",
    "mysql": "^2.8.0",
    "node-file-parser": "^1.0.1",
    "node-uuid": "^1.4.7",
    "passport": "^0.2.2",
    "passport-http": "^0.2.2",
    "passport-restify": "^1.0.0",
    "q": "^1.4.1",
    "react": "^0.14.2",
    "react-dom": "^0.14.2",
    "react-redux": "^4.0.0",
    "redux": "^3.0.4",
    "redux-logger": "^2.0.4",
    "redux-promise": "^0.5.0",
    "redux-thunk": "^1.0.0",
    "redux-undo": "^0.5.0",
    "restify": "^3.0.3",
    "sequelize": "^3.13.0",
    "socket.io": "^1.3.7",
    "typr": "^0.2.1",
    "vantage": "^1.5.1",
    "vantage-command-proxy": "^1.0.0",
    "vantage-repl-mysql": "^1.0.0",
    "winston": "^1.0.1"
  },
  "devDependencies": {
    "babel": "^6.3.13",
    "babel-cli": "^6.3.15",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "chai": "^3.3.0",
    "chai-immutable": "^1.3.0",
    "css-loader": "^0.20.2",
    "fs.extra": "^1.3.2",
    "gulp": "^3.9.0",
    "gulp-babel": "^6.1.1",
    "gulp-change": "^1.0.0",
    "gulp-changed": "^1.3.0",
    "gulp-concat": "^2.6.0",
    "gulp-sourcemaps": "^1.6.0",
    "mocha": "^2.3.3",
    "requirejs": "^2.1.20",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.1"
  }
}
