{
  "name": "devapt-server-node",
  "version": "1.0.0",
  "description": "Devapt REST API server",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "test": "mocha --compilers js:babel/register --require ./test/test_helper.js --recursive",
    "test:watch": "npm run test -- --watch",
    "build": "webpack --progress --colors",
    "build:watch": "webpack --progress --colors --watch",
    "start:dev": "webpack-dev-server ./entry --progress --colors --hot --inline",
    "build:client": "./node_modules/.bin/babel client/src/js --out-file dist/devapt_client_babel.js",
    "build:common": "./node_modules/.bin/babel common --out-file dist/devapt_common_babel.js",
    "build:server": "./node_modules/.bin/babel server --out-file dist/devapt_server_babel.js"
  },
  "keywords": [
    "server",
    "api",
    "rest",
    "devapt",
    "toolkit",
    "model"
  ],
  "author": "Luc BORIES",
  "license": "Apache 2",
  "dependencies": {
    "MD5": "^1.3.0",
    "babel-core": "^5.8.22",
    "debug": "^2.2.0",
    "epilogue": "^0.5.1",
    "fbemitter": "~2.0.0",
    "immutable": "^3.7.5",
    "immutable-js": "~0.3.1-6",
    "ini": "^1.3.4",
    "mysql": "^2.8.0",
    "node-file-parser": "^1.0.1",
    "passport": "^0.2.2",
    "passport-http": "^0.2.2",
    "passport-restify": "^1.0.0",
    "q": "^1.4.1",
    "redux": "^3.0.4",
    "redux-logger": "^2.0.4",
    "redux-promise": "^0.5.0",
    "redux-thunk": "^1.0.0",
    "redux-undo": "^0.5.0",
    "restify": "^3.0.3",
    "sequelize": "^3.5.1",
    "typr": "^0.2.1",
    "winston": "^1.0.1"
  },
  "devDependencies": {
    "babel": "^5.8.29",
    "babel-loader": "^5.3.2",
    "chai": "^3.3.0",
    "chai-immutable": "^1.3.0",
    "css-loader": "^0.20.2",
    "fs.extra": "^1.3.2",
    "mocha": "^2.3.3",
    "requirejs": "^2.1.20",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.1"
  }
}
